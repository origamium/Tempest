    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "jest": "29.6.1",
    "npm-run-all": "4.1.5",
    "prettier": "3.0.0",
    "stylelint": "15.10.1",
    "stylelint-config-recess-order": "4.3.0",
    "stylelint-config-recommended": "13.0.0",
    "stylelint-config-styled-components": "0.1.1",
