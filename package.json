    "@types/react": "18.2.14",
    "@types/react-beautiful-dnd": "13.1.4",
    "@types/react-dom": "18.2.7",
    "@types/react-dropzone": "5.1.0",
    "@types/react-redux": "7.1.25",
    "@types/react-virtualized": "9.21.22",
