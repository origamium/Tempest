{
  "name": "tempestclient",
  "version": "0.1.0",
  "engines": {
    "node": "18.x"
  },
  "dependencies": {
    "@material-ui/core": "4.11.3",
    "@material-ui/icons": "4.11.2",
    "array-move": "3.0.1",
    "deepmerge": "4.2.2",
    "form-urlencoded": "4.2.1",
    "nanoid": "3.1.31",
    "path-to-regexp": "6.2.0",
    "query-string": "6.14.1",
    "react": "17.0.1",
    "react-beautiful-dnd": "13.0.0",
    "react-dom": "17.0.1",
    "react-dropzone": "11.3.1",
